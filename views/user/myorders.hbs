<body>

    <div class="order-container"> 
        <button class="mobile-filter-button material-icons-outlined" onclick="orderFilter()">filter_alt</button>
        <ul class="bread-crump">
            <li>Home</li>
            <span class="material-symbols-outlined">
				arrow_right
				</span>
            <li>Account</li>
            <span class="material-symbols-outlined">
				arrow_right
				</span>
            <li>Wishlist</li>
        </ul>
            <div class="filter-container">
                <div class="nav-header">
                    <i class="material-icons-outlined" onclick="closeOrder()">arrow_back</i>
                </div>
                <div class="filter-header">
                 
                    <h3>Filters</h3>
                </div>
                <div class="filter-content">
                    <div class="filter-items">
                        <input type="checkbox">
                        <p>On the Way</p>
                    </div>
                    <div class="filter-items">
                        <input type="checkbox">
                        <p>Delivered</p>
                    </div>
                    <div class="filter-items">
                        <input type="checkbox">
                        <p>returned</p>
                    </div>
                    <div class="filter-items">
                        <input type="checkbox">
                        <p>cancelled</p>
                    </div>
                </div>
            </div>
            <div class="cart-items--wrapper">
               

                <div class="dashboard--container">
                  
                    <div class="cart-content">
                        <div class="cart--header">
                            <h2 class="cart--heading">My orders ( 5 )</h2>
                            <div class="filter-date">
                                <select name="filter" id="">
                                    <option value="Past 6 months">Past 6 months</option>
                                    <option value="Past 6 months">Past 6 months</option>
                                    <option value="Past 6 months">Past 6 months</option>
                                    <option value="Past 6 months">Past 6 months</option>
                                </select>
                            </div>
                        </div>
                        <div class="cart--card">
                            <div class="cart-item">
                                <img src="/assets/images/ck.jpg" class="cart-item--image" alt="">
                                <div class="cart-item--body">
                                    <div class="cart-product">
                                        <h4 class="cart-item--title">Wajah Ceramic Vase</h4>
                                        <input type="checkbox" name="cart--check" class="cart--check">
                                    </div>
                                    <strong class="price">AED 500.00</strong>
                                    <p class="stock-status">In Stock</p>
                                    <div class="quantity--wrapper">
                                        <strong class="qty--title">QTY: </strong>
                                        <span class="material-icons-outlined quantity--changer">remove</span>
                                        <span class="quantity">2</span>
                                        <span class="material-icons-outlined quantity--changer">add</span>
                                    </div>
                                    <div class="delete-and-total--wrapper">
                                        <a href="#" class="btn delete--btn">Delete</a>
                                        <strong class="total--price">Total: <span class="price">500.00</span></strong>
                                    </div>
                                </div>
                            </div>
                        </div>
            
